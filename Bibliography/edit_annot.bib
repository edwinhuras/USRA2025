	shorttitle = {The {{Whole}} Is {{Greater Than}} the {{Sum}} of {{Its Parts}}},
	title = {The {{Whole}} Is {{Greater Than}} the {{Sum}} of {{Its Parts}}: {{Large-scale Phenomena Arising}} from {{Small-scale Biophysical Processes}}},
